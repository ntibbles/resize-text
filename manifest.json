{
  "manifest_version": 3,
  "name": "Resize Text By Equa11y",
  "version": "1.0",
  "description": "An extension to quickly and easily resize the browser text up to 200%",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}